<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/CF1RPRF01E" version="2019.1.100" xmlns="http://www.lmonte.com/besm/CF1RPRF01E" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:complexType name="CF1RPRF01E">
      <xsd:annotation>
         <xsd:documentation>Residential Performance Compliance Method</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">General Information</xsd:documentation>
               <xsd:appinfo source="CBECCresXML">
                  <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A01_ProjectName" type="com:ProjectName">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Name</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:Name</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_RunDescription" type="comp:RunDescription">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Run Title</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:Description</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_ProjectStreetAddress" type="comp:ProjectStreetAddress">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:Address</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_ProjectCity" type="comp:ProjectCity" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">City</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:City</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_T24StandardsCodeYear" type="comp:T24StandardsCodeYear">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Standards Version</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:StandardsVersion</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_ProjectZipcode" type="comp:ProjectZipcode" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip code</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:ZipCode</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_ComplianceSoftwareVersion" type="comp:ComplianceSoftwareVersion">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Software Version</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:SoftwareVersion</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_ClimateZone" type="com:ClimateZone">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:ClimateZone</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>Need condition to truncate this text field value to first 4 characters.  See CF1R Rendering Model.xsl</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A09_Azimuth" type="dtyp:Azimuth">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Front Orientation (deg/ Cardinal)</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>Project:FrontOrientation</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A09_CardinalOrientation">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Front Orientation (deg/ Cardinal)</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>Project:not found</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="com:CardinalOrientation">
                              <xsd:enumeration value="AllOrientations"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="A10_ResidentialLowriseBuildingType" type="com:ResidentialLowriseBuildingType">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Type</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>0=SingleFamily;1=LowriseMultiFamily</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A11_DwellingUnitCount" type="com:DwellingUnitCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Dwelling Units</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:NumDwellingUnits</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A12_ProjectScopePerformance" type="comp:ProjectScopePerformance">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Scope</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>RunScope 1=NewConstruction;2=AdditionAlteration; IsAddAlone 1=AdditionOnly</dtyp:termConditions>
                        </xsd:appinfo>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:RunScope</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>RunScope 1=NewConstruction;2=AdditionAlteration; IsAddAlone 1=NewAdditionOnly</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A13_BedroomCount" type="com:BedroomCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Bedrooms</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A14_NewConditionedFloorArea" type="bld:NewConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">New Cond. Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:CondFloorArea</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A15_TotalStoryCount" type="com:TotalStoryCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Stories</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:NumStories</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A16_ExistingConditionedFloorArea" type="bld:ExistingConditionedFloorArea">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Existing Cond. Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>Project:BldgSlabFloorArea</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A16_NotApplicableMessage" type="comp:NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Existing Cond. Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="A17_UFactorFenestrationAverage" type="com:UFactorFenestrationAverage">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fenestration Average U-factor</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A18_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Cond. Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:AdditionCFA</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>Also Project:CondFloorArea. Which to use?</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A19_WindowFloorRatio" type="env:WindowFloorRatio">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Glazing Percentage (%)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Value displayed: (100 * WindowFloorRatio) %</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:CondWinAreaCFARat</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>Need calculation condition, See CF1R Rendering Model.xsl: sum(//Model[@Name='Proposed']/Proj/Zone/RptTotCondZoneWinArea) div sum(//Model[@Name='Proposed']/Proj/Zone/FloorArea)</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A20_NaturalGasOnSite" type="com:NaturalGasOnSite">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A21_FirstFloorToHighestCeilingHeight" type="com:FirstFloorToHighestCeilingHeight" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Value is not required If this is a multifamily project; Value from IAQ dialog box, Maximum Vertical Distance.  Used in CF2R-Mch27a</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A22_SingleFamilyDwellingAttached" type="com:SingleFamilyDwellingAttached">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A22_NotApplicableMessage" type="comp:NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A23_AccessoryDwellingUnitIncluded" type="com:AccessoryDwellingUnitIncluded">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A23_NotApplicableMessage" type="comp:NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A24_ADUBedroomCount" type="com:ADUBedroomCount">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">ADU Bedroom Count</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A24_NotApplicableMessage" type="comp:NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">ADU Bedroom Count</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A25_ADUConditionedFloorArea" type="com:ADUConditionedFloorArea">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">ADU Conditioned Floor Area</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A25_NotApplicableMessage" type="comp:NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">ADU Conditioned Floor Area</xsd:documentation>
                           <xsd:appinfo source="CBECCresXML">
                              <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="A26_HERSCount" type="comp:HERSCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A27_TotalConditionedVolume" type="com:TotalConditionedVolume">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A28_CompMgrVersion" type="comp:CompMgrVersion">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A29_GasType" type="com:GasType">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">User Defined Fields</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:element name="AUserDefined_FieldName" type="com:FieldName"/>
                     <xsd:element name="AUserDefined_FieldValue" type="com:FieldValue"/>
                     <xsd:element name="AUserDefined_FieldValueType" type="com:FieldValueType"/>
                  </xsd:sequence>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Addition Alone Project Analysis Parameters</xsd:documentation>
               <xsd:appinfo source="CBECCresXML">
                  <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B01_ExistingConditionedFloorArea" type="bld:ExistingConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Existing Area (excl. new addition) (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:AddAloneExistArea</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02_BuildingAdditionalConditionedFloorArea" type="bld:BuildingAdditionalConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Addition Area (excl. existing) (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:AddAloneAddedArea</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Area  (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:AddAloneTotalArea</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B04_BedroomsExistingCount" type="com:BedroomsExistingCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Existing Bedrooms</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:NumBedrooms</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B05_BedroomsAdditionCount" type="com:BedroomsAdditionCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Addition Bedrooms</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>Project:NumAddBedrooms</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B06_BedroomCount" type="com:BedroomCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Bedrooms</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>Need calculation condition, See CF1R Rendering Model.xsl: NumBedrooms + NumAddBedrooms</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">User Defined Fields</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:element name="BUserDefined_FieldName" type="com:FieldName"/>
                     <xsd:element name="BUserDefined_FieldValue" type="com:FieldValue"/>
                     <xsd:element name="BUserDefined_FieldValueType" type="com:FieldValueType"/>
                  </xsd:sequence>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">COMPLIANCE RESULTS</xsd:documentation>
               <xsd:appinfo source="CBECCresXML">
                  <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C01_ResidentialPerformanceComplianceResult" type="comp:ResidentialPerformanceComplianceResult">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">01</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>EUseSummary:PassFail</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>Need logic condition to select Standard:Proj:EUseSummary for this field vs Proposed for fileds C02 and C03</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C02_ResidentialPerformanceHERS_VerificationRequired" type="comp:ResidentialPerformanceHERS_VerificationRequired">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">02</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>EUseSummary:</dtyp:CBECCresXMLterm>
                           <dtyp:termConditions>Need to include multiple EUseSummary properties here: HERSCool, HERSProj, HERSDHW, HERSDist</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C03_ResidentialPerformanceSpecialFeatures" type="comp:ResidentialPerformanceSpecialFeatures">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">03</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This field value is only displayed if ResidentialPerformanceSpecialFeatures is true.  If true the display text is This building incorporates one or more Special Features shown below.</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>EUseSummary:SpeclFtr</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">User Defined Fields</xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:element name="C03AfterUserDefined_FieldName" type="com:FieldName"/>
