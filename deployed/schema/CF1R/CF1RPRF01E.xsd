                  <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                  <dtyp:termConditions>XSLT uses DwellUnitRpt object instead of DwellUnit object, but it is not listed in Sharepoint.</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableDwellingUnits">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="H01_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnit:Name</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>XSLT uses DwellUnitRpt object instead of DwellUnit object, but it is not listed in Sharepoint.</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H02_ResidentialDwellingUnitDescriptor" type="com:ResidentialDwellingUnitDescriptor">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnitRpt:DwellUnitTypeRef</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>XSLT uses DwellUnitRpt object instead of DwellUnit object, but it is not listed in Sharepoint.</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H03_ThermalZoneName" type="com:ThermalZoneName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This value should match one of a value in the Zone Information section column 01</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnitRpt:ZoneRef</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>XSLT uses DwellUnitRpt object instead of DwellUnit object, but it is not listed in Sharepoint.</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">User Defined Fields</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:element name="HUserDefined_FieldName" type="com:FieldName"/>
                                       <xsd:element name="HUserDefined_FieldValue" type="com:FieldValue"/>
                                       <xsd:element name="HUserDefined_FieldValueType" type="com:FieldValueType"/>
                                    </xsd:sequence>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_I" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">DWELLING UNIT TYPES</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if Building type in General Information section equals LowriseMultiFamily</xsd:documentation>
               <xsd:appinfo source="CBECCresXML">
                  <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                  <dtyp:termConditions>Need logic to loop over all instances of //Model[@Name='Proposed']/DwellUnitType</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableDwellingUnitType">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="I01_ResidentialDwellingUnitDescriptor" type="com:ResidentialDwellingUnitDescriptor">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnitType:Name</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I02_DwellingUnitConditionedFloorArea" type="com:DwellingUnitConditionedFloorArea">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">CFA (ft<d:sup>2</d:sup>)</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnitType:CondFloorArea</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I03_BedroomCount" type="com:BedroomCount">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Number of Bedrooms</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnitType:NumBedrooms</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I04_DwellingUnitsIdenticalCount" type="comp:DwellingUnitsIdenticalCount">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Number in Building</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>XSLT uses logic: 'Proposed']/Proj/Zone/DwellUnit[DwellUnitTypeRef=$dwellunit]/Count</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I05_DwellingUnitSpaceConditioningSystemName" type="hvac:DwellingUnitSpaceConditioningSystemName" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Space Conditioning Systems Assigned</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Display dwelling unit name which should be a value from Dwelling Unit Information section I01 followed by  the name of assigned space conditioning systems</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnitType:SCSysRptRef[]</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>Need logic to concatenate over index values of SCSysRptRef[]</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I06_DomesticHotWaterSystemName" type="comp:DomesticHotWaterSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">DHW System Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnitType:DHWSysRef</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I07_IAQ_VentilationFanName" type="hvac:IAQ_VentilationFanName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">IAQ Vent Fan Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>DwellUnitType:IAQOption</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>XSLT uses IAQFanRef, but not found in Analysis Results file</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">User Defined Fields</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:element name="IUserDefined_FieldName" type="com:FieldName"/>
                                       <xsd:element name="IUserDefined_FieldValue" type="com:FieldValue"/>
                                       <xsd:element name="IUserDefined_FieldValueType" type="com:FieldValueType"/>
                                    </xsd:sequence>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">OPAQUE SURFACES</xsd:documentation>
               <xsd:appinfo source="CBECCresXML">
                  <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                  <dtyp:termConditions>Proj/Zone/ExtWall, IntWall, CeilingBelowAttic, ExteriorFloor, FloorOverCrawl, InteriorFloor; Proj/Garage/ExtWall, , IntWall, CeilingBelowAttic, ExteriorFloor, FloorOverCrawl, InteriorFloor, InteriorCeiling, UndWall</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableOpaqueSurfaces">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="J01_PartitionName" type="env:PartitionName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>ExtWall:Name</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>IntWall:Name</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>CeilingBelowAttic:Name</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J02_ThermalZoneName" type="com:ThermalZoneName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>Zone:Name</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>Need condition to relate selected opaque surface to its parent Zone, or could we use "../" syntax?</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J03_ConstructionAssemblyNamePerformanceRes" type="comp:ConstructionAssemblyNamePerformanceRes">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Construction</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>ExtWall:Construction</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>IntWall:Construction</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="J04_Azimuth" type="dtyp:Azimuth" minOccurs="0">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Azimuth</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If opaque surface is interior wall or a  ceiling, no value is required and "n/a" is displayed in report</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>ExtWall:Azimuth</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="J04_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Azimuth</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If opaque surface is interior wall or a  ceiling, no value is required and "n/a" is displayed in report</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="J05_RelativePosition" type="com:RelativePosition" minOccurs="0">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Orientation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If opaque surface is interior wall or a  ceiling, no value is required and "n/a" is displayed in report</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>ExtWall:Orientation</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="J05_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Orientation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If opaque surface is interior wall or a  ceiling, no value is required and "n/a" is displayed in report</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="J06_PartitionGrossArea" type="env:PartitionGrossArea">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Gross Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>ExtWall:Area</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>IntWall:Area</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>CeilingBelowAttic:Area</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="J07_PartitionWindowDoorArea" type="com:PartitionWindowDoorArea" minOccurs="0">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Window and Door Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If there are no windows or doors in the partition  no value is required and "n/a" is displayed in report</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>ExtWall:ChildAreaSum</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>IntWall:ChildAreaSum</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>CeilingBelowAttic:ChildAreaSum</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="J07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Window and Door Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If there are no windows or doors in the partition  no value is required and "n/a" is displayed in report</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="J08_Tilt" type="dtyp:Tilt" minOccurs="0">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tilt (deg)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If opaque surface is interior wall or a  ceiling, no value is required and "n/a" is displayed in report</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>ExtWall:Tilt</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="J08_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tilt (deg)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If opaque surface is interior wall or a  ceiling, no value is required and "n/a" is displayed in report</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="J09_StatusEAA" type="com:StatusEAA" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This field is required if ProjectScopePerformance in GENERAL INFORMATION == AdditionAlteration And changes involve opaque surfaces.</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>ExtWall:Status</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>I don't see Status field in XSLT or PDF report.  See Calc&amp;amp;Rules condition.</dtyp:termConditions>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>IntWall:Status</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>I don't see Status field in XSLT or PDF report.  See Calc&amp;amp;Rules condition.</dtyp:termConditions>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>CeilingBelowAttic:Status</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>I don't see Status field in XSLT or PDF report.  See Calc&amp;amp;Rules condition.</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J10_VerifiedExistingCondition" type="comp:VerifiedExistingCondition" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Verified Existing Condition</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This field is required if ProjectScopePerformance in GENERAL INFORMATION == AdditionAlteration And changes involve opaque surfaces. 
 If J09_StatusEEA == New result =NA; Else  If StatusEEA  == Altered result = Yes; Else If StatusEEA  == Existing result = No.</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>ExtWall:IsExisting</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>Not sure is this should be IsExisting or IsVerified or combination</dtyp:termConditions>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>IntWall:IsExisting</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>Not sure is this should be IsExisting or IsVerified or combination</dtyp:termConditions>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>CeilingBelowAttic:IsExisting</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>Not sure is this should be IsExisting or IsVerified or combination</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J11_ConstructionAssemblyNamePerformanceRes" type="comp:ConstructionAssemblyNamePerformanceRes" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Existing Construction</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This field is required if ProjectScopePerformance in GENERAL INFORMATION == AdditionAlteration And changes involve opaque surfaces And  If StatusEEA  == Altered</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
