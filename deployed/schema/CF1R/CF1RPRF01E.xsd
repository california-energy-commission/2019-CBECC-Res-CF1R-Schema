         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">DWELLING UNIT INFORMATION</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if Building type in General Information section equals LowriseMultiFamily</xsd:documentation>
