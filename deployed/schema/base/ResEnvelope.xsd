<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:com="http://www.lmonte.com/besm/com"
            xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
            xmlns:env="http://www.lmonte.com/besm/env"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            attributeFormDefault="unqualified"
            elementFormDefault="qualified"
            targetNamespace="http://www.lmonte.com/besm/env"
            version="2016.1.010">
   <xsd:import namespace="http://www.lmonte.com/besm/com"
               schemaLocation="ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp"
               schemaLocation="DataTypes.xsd"/>
   <xsd:simpleType name="AirInfiltrationRate">
      <xsd:annotation>
         <xsd:documentation>Uncontrolled inward air leakage from outside a building or unconditioned space, including leakage through cracks and interstices, around windows and doors, and through any other exterior or demising partition or pipe or duct penetration.  Units: ft3 per min ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Air Infiltration Rate</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AssemblyLayers">
      <xsd:restriction base="xsd:string"/>
   </xsd:simpleType>
   <xsd:complexType name="Attic">
      <xsd:annotation>
         <xsd:documentation>An enclosed unconditioned space directly below the roof deck and above the ceiling beams.  Standards: 2013 NR ACM: Attic is not a Building Descriptor.  Attic is referenced as an example of unoccupied and unconditioned spaces in Section 2.2.4
 
2013 JA 4 has separate tables for assigning UFactor to roofs with attics:  Table 4.2.1 U-factors of Wood Framed Attic Roofs and Table 4.2.4 U-factors of Metal Framed Attic Roofs.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="atticFloorArea" type="env:AtticFloorArea"/>
         <xsd:element name="atticNetFreeAreaPerVent" type="env:AtticNetFreeAreaPerVent"/>
         <xsd:element name="atticInstalledTotalNetFreeArea"
                      type="env:AtticInstalledTotalNetFreeArea"/>
         <xsd:element name="atticTotalNetFreeAreaPerVentType"
                      type="env:AtticTotalNetFreeAreaPerVentType"/>
         <xsd:element name="atticVentCountPerType" type="env:AtticVentCountPerType"/>
         <xsd:element name="atticVentType" type="env:AtticVentType"/>
         <xsd:element name="atticUpperVentMinimumNetFreeAreaRequired"
                      type="env:AtticUpperVentMinimumNetFreeAreaRequired"/>
         <xsd:element name="atticVentFreeAreaRequired" type="env:AtticVentFreeAreaRequired"/>
         <xsd:element name="atticVentilationAreaInstalled"
                      type="env:AtticVentilationAreaInstalled"/>
         <xsd:element name="atticVentilationAreaRequired"
                      type="env:AtticVentilationAreaRequired"/>
         <xsd:element name="insulationPlacement" type="env:InsulationPlacement"/>
         <xsd:element name="insulationRValue" type="env:InsulationRValue"/>
         <xsd:element name="insulationType" type="env:InsulationType"/>
      </xsd:sequence>
      <xsd:attribute name="atticName" type="env:AtticName" use="required"/>
      <xsd:attribute name="improvementStatus"
                     type="com:ImprovementStatus"
                     use="required"/>
   </xsd:complexType>
   <xsd:simpleType name="AtticAllVentMinimumNetFreeAreaRequired">
      <xsd:annotation>
         <xsd:documentation>  Units: in2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Minimum Net Free Area required for all attic vents</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticFloorArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticInstalledTotalNetFreeArea">
      <xsd:annotation>
         <xsd:documentation>  Units: in2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticName">
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticNetFreeAreaPerVent">
      <xsd:annotation>
         <xsd:documentation>  Units: in2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticTotalNetFreeAreaPerVentType">
      <xsd:annotation>
         <xsd:documentation>  Units: in2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Total Net Free Area per attic vent type</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticUpperVentMinimumNetFreeAreaRequired">
      <xsd:annotation>
         <xsd:documentation>  Units: in2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Minimum Net Free Area required for attic upper  vents</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticUpperVentsTotalNetFreeAreaInstalled">
      <xsd:annotation>
         <xsd:documentation>  Units: in2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Total   Net Free Area of installed attic upper vents</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticVentCountPerType">
      <xsd:annotation>
         <xsd:documentation>  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticVentFreeAreaInstalled">
      <xsd:annotation>
         <xsd:documentation>This is the minimum amount of floor area in an attic that is a required to be ventilated by a whole house fan  Units: in2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticVentFreeAreaRequired">
      <xsd:annotation>
         <xsd:documentation>This is the amount of floor area in an attic that is allowed to be free of ventilation and is calculated by dividing the required fan ventilation for the whole house by the factor of 375 CFM  Units: in2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticVentLinearFeet">
      <xsd:annotation>
         <xsd:documentation>  Units: ft</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal1PlaceNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticVentType">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Attic Vent Type</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="AtticVentOther">Other</dtyp:displayterm>
            <dtyp:displayterm value="EaveVent">Eave vent</dtyp:displayterm>
            <dtyp:displayterm value="EyebrowVent">Eyebrow vent</dtyp:displayterm>
            <dtyp:displayterm value="RidgeVent">Ridge Vent</dtyp:displayterm>
            <dtyp:displayterm value="RoundVent">Round vent</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="AtticVentOther"/>
         <xsd:enumeration value="EaveVent"/>
         <xsd:enumeration value="EyebrowVent"/>
         <xsd:enumeration value="RidgeVent"/>
         <xsd:enumeration value="RoundVent"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="AtticVentilationAreaInstalled">
      <xsd:annotation>
         <xsd:documentation>This is the minimum amount of floor area in an attic that is a required to be ventilated by a whole house fan  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="AtticVentilationAreaRequired">
      <xsd:annotation>
         <xsd:documentation>This is the minimum amount of floor area in an attic that is a required to be ventilated by a whole house fan  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:complexType name="BuildingEnvelope">
      <xsd:annotation>
         <xsd:documentation>Contains data to define all building Partitions that enclose the project conditioned space. The Envelope may be made up of all exterior partitions such as Exterior Walls, Exterior Floor and Roof or it may include demising partitions, if the project is an addition or alteration, or if it contains unconditioned space.  Standards: 2013 NR ACM: Used in Section 1.6.2 to classify Scope of Compliance Calculations (e.g., Envelope Only, Envelope and Lighting, etc.).  
T24: Building Envelope is the ensemble of exterior and demising partitions of a building that enclose conditioned space.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="totalGlazingAreaAllOrientations"
                      type="env:TotalGlazingAreaAllOrientations"/>
         <xsd:element name="DoorConstructions" type="env:DoorConstructions"/>
         <xsd:element name="ExteriorFloor" type="env:ExteriorFloor"/>
         <xsd:element name="ExteriorWall" type="env:ExteriorWall"/>
         <xsd:element name="Fenestration" type="env:Fenestration"/>
         <xsd:element name="Roof" type="env:Roof"/>
      </xsd:sequence>
      <xsd:attribute name="envelopeName" type="env:EnvelopeName" use="required"/>
      <xsd:attribute name="improvementStatus"
                     type="com:ImprovementStatus"
                     use="required"/>
   </xsd:complexType>
   <xsd:simpleType name="CRRCProductID">
      <xsd:annotation>
         <xsd:documentation>The CRRC product identification number for the roofing product</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token"/>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_AgedEmittance">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated 3-year aged thermal emittance of a roofing material.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_AgedReflectance">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated 3-year aged solar reflectance of a roofing material.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_AgedSRI">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated 3-year aged Solar Reflective Index (SRI) of a roofing material.The SRIis a measure defined by the Lawrence Berkeley National Laboratory as the roof's ability to reject solar heat, as shown by a small temperature rise.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_InitialEmittance">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated initial thermal emittance of a roofing material.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_InitialReflectance">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated initial solar reflectance of a roofing material.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_InitialSRI">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated initial Solar Reflective Index (SRI) of a roofing material.The SRIis a measure defined by the Lawrence Berkeley National Laboratory as the roof's ability to reject solar heat, as shown by a small temperature rise.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="CavityInsulation">
      <xsd:annotation>
         <xsd:documentation>  Units: h ft2 F per Btu</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="CavityInsulationNR">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Cavity Insulation</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant"/>
   </xsd:simpleType>
   <xsd:simpleType name="CavityInsulationOther">
      <xsd:annotation>
         <xsd:documentation>Short phrase descirbing cavity insulation that is non-standard or unique.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token"/>
   </xsd:simpleType>
   <xsd:simpleType name="CavityInsulationRes">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Cavity Insulation</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Other"/>
            <dtyp:displayterm value="R0">R 0</dtyp:displayterm>
            <dtyp:displayterm value="R11">R 11</dtyp:displayterm>
            <dtyp:displayterm value="R13">R 13</dtyp:displayterm>
            <dtyp:displayterm value="R15">R 15</dtyp:displayterm>
            <dtyp:displayterm value="R19">R 19</dtyp:displayterm>
            <dtyp:displayterm value="R21">R 21</dtyp:displayterm>
            <dtyp:displayterm value="R22">R 22</dtyp:displayterm>
            <dtyp:displayterm value="R23">R 23</dtyp:displayterm>
            <dtyp:displayterm value="R24">R 24</dtyp:displayterm>
            <dtyp:displayterm value="R25">R 25</dtyp:displayterm>
            <dtyp:displayterm value="R30">R 30</dtyp:displayterm>
            <dtyp:displayterm value="R38">R 38</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Other"/>
         <xsd:enumeration value="R0"/>
         <xsd:enumeration value="R11"/>
         <xsd:enumeration value="R13"/>
         <xsd:enumeration value="R15"/>
         <xsd:enumeration value="R19"/>
         <xsd:enumeration value="R21"/>
         <xsd:enumeration value="R22"/>
         <xsd:enumeration value="R23"/>
         <xsd:enumeration value="R24"/>
         <xsd:enumeration value="R25"/>
         <xsd:enumeration value="R30"/>
         <xsd:enumeration value="R38"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="Ceiling">
      <xsd:annotation>
         <xsd:documentation>A building partition forming the interior, overhead boundary of a space.  Standards: 2013 NR ACM:  Ceiling is not a Building Descriptor 
JA1:  Ceiling is the interior upper surface of a space separating it from an attic, plenum, indirectly or directly conditioned space or the roof assembly,
 2013 JA4 tables in section 4.2 have rules regarding Ceiling properties</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="env:Partition">
            <xsd:sequence>
               <xsd:element name="ceilingConstructionType" type="env:CeilingConstructionType"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="CeilingConstructionType">
      <xsd:annotation>
         <xsd:documentation>One of several general types of ceiling construction.</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Ceiling Construction Type</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Suspended">Suspended</dtyp:displayterm>
            <dtyp:displayterm value="TBar">T-bar</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Suspended"/>
         <xsd:enumeration value="TBar"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CeilingInsulation">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Ceiling Insulation</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="R30">R-30</dtyp:displayterm>
            <dtyp:displayterm value="R38">R-38</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="R30"/>
         <xsd:enumeration value="R38"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="ConcreteRaisedConstructionAssemblyFloor">
      <xsd:sequence>
         <xsd:element name="insulationContinuousRValue" type="env:InsulationContinuousRValue"/>
         <xsd:element name="insulationPlacement" type="env:InsulationPlacement"/>
         <xsd:element name="partitionThickness" type="env:PartitionThickness"/>
         <xsd:element name="slabType" type="env:SlabType"/>
         <xsd:element name="uFactorLimit" type="com:UFactorLimit"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:simpleType name="ConcreteThickness">
      <xsd:annotation>
         <xsd:documentation>Thickness of concrete topping on metal deck roofs or reinforced concrete roofs.  Evaluated in order to assign UFactor.  Units: in</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Roof Concrete Thickness</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructedShadingType">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Shading Type</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Overhang">Overhang</dtyp:displayterm>
            <dtyp:displayterm value="VerticalFin">Vertical fin</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Overhang"/>
         <xsd:enumeration value="VerticalFin"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="ConstructionAssembliesFenestration">
      <xsd:sequence>
         <xsd:element name="FenestrationConstructionsSkylight"
                      type="env:FenestrationConstructionsSkylight"/>
         <xsd:element name="FenestrationConstructionsVertical"
                      type="env:FenestrationConstructionsVertical"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ConstructionAssembly">
      <xsd:annotation>
         <xsd:documentation>  Standards: 2013 NR ACM:  Section 5.5.2 Construction Assemblies For California Compliance for the proposed design shall be selected from Joint Appendix 4.  Choices from Joint Appendix 4 are specified for the baseline building, except as specified in this manual.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="cFactor" type="com:CFactor"/>
         <xsd:element name="constructionAssemblyRValue" type="env:ConstructionAssemblyRValue"/>
         <xsd:element name="constructionAssemblyThickness"
                      type="env:ConstructionAssemblyThickness"/>
         <xsd:element name="constructionAssemblyUFactor"
                      type="env:ConstructionAssemblyUFactor"/>
         <xsd:element name="fFactor" type="com:FFactor"/>
         <xsd:element name="Layers" type="env:Layers" minOccurs="0" maxOccurs="1"/>
         <xsd:element name="Materials" type="env:Materials"/>
      </xsd:sequence>
      <xsd:attribute name="manufacturerBrand"
                     type="env:ManufacturerBrand"
                     use="required"/>
      <xsd:attribute name="constructionSpecificationMethod"
                     type="env:ConstructionSpecificationMethod"
                     use="required"/>
   </xsd:complexType>
   <xsd:simpleType name="ConstructionAssemblyRValue">
      <xsd:annotation>
         <xsd:documentation>  Units: h ft2 F per Btu</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructionAssemblyThickness">
      <xsd:annotation>
         <xsd:documentation>The thickness of a  construction assembly thickness measured in inches  Units: in</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructionAssemblyUFactor">
      <xsd:annotation>
         <xsd:documentation>The overall coefficient of thermal transmittance through a construction assembly. It includes air film resistance at both surfaces.  Units: Btu per h ft2 F</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructionSpecificationMethod">
      <xsd:annotation>
         <xsd:documentation>The method of describing a construction assembly. For California compliance, only the Layers approach is used. Other inputs facilitate specification of the final method used in simulation.</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Construction Specification Method</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Layers">Layers</dtyp:displayterm>
            <dtyp:displayterm value="PredefinedConstructionAssembly">Predefined construction assembly</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Layers"/>
         <xsd:enumeration value="PredefinedConstructionAssembly"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="CrawlSpace">
      <xsd:annotation>
         <xsd:documentation>An enclosed space under the first floor of a building adjacent to the ground.  Floor construction assemblies with crawl spaces have different Floor U-factor tables in JA IV.  In residential buildings there are additional rules that involve the crawl space.  Standards: 2013 NR ACM: Not a Building Descriptor.  Only referenced as an example of unoccupied and unconditioned spaces in Section 2.2.4

In 2013 JA IV there are tables for floors with and with out crawl spaces.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence/>
      <xsd:attribute name="crawlSpaceName" type="env:CrawlSpaceName" use="required"/>
   </xsd:complexType>
   <xsd:simpleType name="CrawlSpaceName">
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:simpleType name="DividerMinimumSpacing">
      <xsd:annotation>
         <xsd:documentation>The measurement of the width of the spacer between double panes in inches.  If it is less than 7/16 in, 0.05 is added to the UFactor.  Units: in</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Divider Minimum Spacing</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="DividerThroughPanes">
      <xsd:annotation>
         <xsd:documentation>A Boolean and if True, indicates that the divider goes through the fenestration pane and this causes the fenestration product U-Factor to be adjusted.</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Divider Through Panes</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="true">Yes</dtyp:displayterm>
            <dtyp:displayterm value="false">No</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:boolean"/>
   </xsd:simpleType>
   <xsd:complexType name="Door">
      <xsd:annotation>
         <xsd:documentation>An opening in the building envelope  or an internal wall, which is opaque or has less than 50% glazing. and provides passage to and from an interior space.  Standards: 2013 NR ACM: Section 5.5.6 Doors (no longer called Exterior Doors).  Table 30 Baseline Building U-factor Criteria for Doors.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="doorArea" type="env:DoorArea"/>
         <xsd:element name="height" type="dtyp:Height"/>
         <xsd:element name="statusEAA" type="com:StatusEAA"/>
         <xsd:element name="width" type="dtyp:Width"/>
         <xsd:element name="xCoordinate" type="dtyp:XCoordinate"/>
         <xsd:element name="yCoordinate" type="dtyp:YCoordinate"/>
         <xsd:element name="FenestrationVertical" type="env:FenestrationVertical"/>
         <xsd:element name="OpeningGeometry" type="env:OpeningGeometry"/>
      </xsd:sequence>
      <xsd:attribute name="doorConstructionName"
                     type="env:DoorConstructionName"
                     use="required"/>
      <xsd:attribute name="doorName" type="env:DoorName" use="required"/>
      <xsd:attribute name="partitionName" type="env:PartitionName" use="required"/>
   </xsd:complexType>
   <xsd:simpleType name="DoorArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="DoorCertificationMethod">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="CECDefaultPerformance">Tables 110.6-A and 110.6-B</dtyp:displayterm>
            <dtyp:displayterm value="NFRCRated">NFRC</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="CECDefaultPerformance"/>
         <xsd:enumeration value="NFRCRated"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="DoorConstruction">
      <xsd:annotation>
         <xsd:documentation>This is a composite definition of a door's construction providing required data to evaluate in assigning UFactor.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="doorCertificationMethod" type="env:DoorCertificationMethod"/>
         <xsd:element name="doorConstructionOperation" type="env:DoorConstructionOperation"/>
         <xsd:element name="uFactor" type="com:UFactor"/>
      </xsd:sequence>
      <xsd:attribute name="doorConstructionName"
                     type="env:DoorConstructionName"
                     use="required"/>
      <xsd:attribute name="doorConstructionType"
                     type="env:DoorConstructionType"
                     use="required"/>
   </xsd:complexType>
   <xsd:simpleType name="DoorConstructionName">
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:simpleType name="DoorConstructionOperation">
      <xsd:annotation>
         <xsd:documentation>An enumeration with values of swinging and non-swinging.  Non-swinging would include sliding doors and roll-up doors.</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Door Type</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="NonSwinging">Non-swinging</dtyp:displayterm>
            <dtyp:displayterm value="Swinging">Swinging</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="NonSwinging"/>
         <xsd:enumeration value="Swinging"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DoorConstructionType">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="InsulatedSingleLayerSectionalMetal">Insulated single layer sectional metal</dtyp:displayterm>
            <dtyp:displayterm value="InsulatedSwingingMetal">Insulated swinging metal</dtyp:displayterm>
            <dtyp:displayterm value="UninsulatedDoubleLayerMetal">Uninsulated double layer metal</dtyp:displayterm>
            <dtyp:displayterm value="UninsulatedSingleLayerMetal">Uninsulated single layer metal</dtyp:displayterm>
            <dtyp:displayterm value="UninsulatedSingleLayerRollupMetal">Uninsulated single layer rollup metal</dtyp:displayterm>
            <dtyp:displayterm value="WoodOther">Wood other</dtyp:displayterm>
            <dtyp:displayterm value="WoodThick">Wood thick</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="InsulatedSingleLayerSectionalMetal"/>
         <xsd:enumeration value="InsulatedSwingingMetal"/>
         <xsd:enumeration value="UninsulatedDoubleLayerMetal"/>
         <xsd:enumeration value="UninsulatedSingleLayerMetal"/>
         <xsd:enumeration value="UninsulatedSingleLayerRollupMetal"/>
         <xsd:enumeration value="WoodOther"/>
         <xsd:enumeration value="WoodThick"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="DoorConstructions">
      <xsd:annotation>
         <xsd:documentation>This is a composite definition of a door's construction providing required data to evaluate in assigning UFactor.  NOTE: Not being assigned to any properties or objects with properties, so should delete when shown Res doesn't need it</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DoorConstruction"
                      type="env:DoorConstruction"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:simpleType name="DoorLocation">
      <xsd:restriction base="xsd:string"/>
   </xsd:simpleType>
   <xsd:simpleType name="DoorName">
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:simpleType name="DynamicGlazingType">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="ChromogenicGlazing">Chromogenic glazing</dtyp:displayterm>
            <dtyp:displayterm value="IntegratedShadingDevice">Integrated shading device</dtyp:displayterm>
            <dtyp:displayterm value="NoDynamicGlazing">None</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="ChromogenicGlazing"/>
         <xsd:enumeration value="IntegratedShadingDevice"/>
         <xsd:enumeration value="NoDynamicGlazing"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="EnvelopAirLeakageRateLimit">
      <xsd:annotation>
         <xsd:documentation>This is the target air leakage for the envelope expressed in CFM at 50 pascals and is used in residential projects.  Units: ft3 per min ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="EnvelopeName">
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:simpleType name="ExistingPlusAddedGlazingAreaAllOrientations">
      <xsd:annotation>
         <xsd:documentation>The total area for all existing and added fenestration glazing  all orientations.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal"/>
   </xsd:simpleType>
   <xsd:simpleType name="ExistingPlusAddedGlazingAreaWest">
      <xsd:annotation>
         <xsd:documentation>The total area for all existing and added west facing fenestration glazing  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal"/>
   </xsd:simpleType>
   <xsd:complexType name="ExteriorFloor">
      <xsd:annotation>
         <xsd:documentation>A Floor that separates the Space from surface ground  Standards: 2013 JA4 4.4 Floors and Slabs contains tables for exterior floors with or without a crawl space.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="env:Partition">
            <xsd:sequence>
               <xsd:element name="exteriorFloorArea" type="env:ExteriorFloorArea"/>
               <xsd:element name="exteriorFloorType" type="env:ExteriorFloorType"/>
               <xsd:element name="statusEAA" type="com:StatusEAA"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="ExteriorFloorArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="ExteriorFloorName">
      <xsd:restriction base="xsd:token"/>
   </xsd:simpleType>
   <xsd:simpleType name="ExteriorFloorType">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Raised">Raised</dtyp:displayterm>
            <dtyp:displayterm value="SlabOnGrade">Slab on grade</dtyp:displayterm>
            <dtyp:displayterm value="SlabRaised">Slab raised</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Raised"/>
         <xsd:enumeration value="SlabOnGrade"/>
         <xsd:enumeration value="SlabRaised"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="ExteriorSurfaceProperties">
      <xsd:annotation>
         <xsd:documentation>Characterize the thermal characteristics of exterior surfaces including emissivity, solar reflectance and roughness which is stored with the Material that makes up the exterior layer.  Standards: 2013 NR ACM: Section 3.2.7 Exterior Wall Surface Properties default values. Section 5.5.3 Roofs contains "Exterior Roof Surface Properties" and Section 5.5.4 Exterior Walls contains "Exterior Wall Surface Properties."  Section 5.11.10 Exterior Surface Properties.  
 2013 T24: Section 110.8.(i) Roofing Products Solar Reflectance and Thermal Emittance.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="exteriorSolarAbsorptance" type="com:ExteriorSolarAbsorptance"/>
         <xsd:element name="exteriorThermalAbsorptance" type="com:ExteriorThermalAbsorptance"/>
         <xsd:element name="exteriorVisibleAbsorptance" type="com:ExteriorVisibleAbsorptance"/>
         <xsd:element name="thermalEmittanceLimit" type="com:ThermalEmittanceLimit"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ExteriorWall">
      <xsd:annotation>
         <xsd:documentation>A Wall that separated the Space from outdoors  Standards: 2013 JA4 4.3 Walls contains tables for exterior walls</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="env:Partition">
            <xsd:sequence>
               <xsd:element name="height" type="dtyp:Height"/>
               <xsd:element name="statusEAA" type="com:StatusEAA"/>
               <xsd:element name="width" type="dtyp:Width"/>
               <xsd:element name="wWR" type="env:WWR"/>
               <xsd:element name="Door" type="env:Door" maxOccurs="unbounded"/>
               <xsd:element name="ExteriorSurfaceProperties" type="env:ExteriorSurfaceProperties"/>
               <xsd:element name="FenestrationVertical" type="env:FenestrationVertical"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="ExteriorWallName">
      <xsd:restriction base="xsd:token"/>
   </xsd:simpleType>
   <xsd:complexType name="ExternalConstructedShadingObject">
      <xsd:annotation>
         <xsd:documentation>Constructed building features, such as overhangs, fins, on the exterior face of the wall, that are documented on the construction documents and shade the glazing or shade the Building.  Standards: 2013 NR ACM This is a building description found in section 5.5.7 Fenestration</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="constructedShadingType" type="env:ConstructedShadingType"/>
         <xsd:element name="externalShadingObjectArea" type="env:ExternalShadingObjectArea"/>
         <xsd:element name="solarReflectance" type="env:SolarReflectance"/>
         <xsd:element name="visibleReflectance" type="env:VisibleReflectance"/>
         <xsd:element name="overhangProjection" type="env:OverhangProjection"/>
         <xsd:element name="verticalFinProjection" type="env:VerticalFinProjection"/>
      </xsd:sequence>
      <xsd:attribute name="externalShadingObjectName"
                     type="env:ExternalShadingObjectName"
                     use="required"/>
   </xsd:complexType>
   <xsd:complexType name="ExternalShadingObject">
      <xsd:annotation>
         <xsd:documentation>Devices or building features, such as overhangs, fins, on the exterior face of the wall, that are documented on the construction documents and shade the glazing or shade the Building.  Standards: 2013 NR ACM This is a building description found in section 5.5.7 Fenestration</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="externalShadingObjectArea" type="env:ExternalShadingObjectArea"/>
         <xsd:element name="solarReflectance" type="env:SolarReflectance"/>
         <xsd:element name="externalShadingObjectType" type="env:ExternalShadingObjectType"/>
         <xsd:element name="visibleReflectance" type="env:VisibleReflectance"/>
         <xsd:element name="overhangProjection" type="env:OverhangProjection"/>
         <xsd:element name="solarHeatGainCoefficientExteriorShading"
                      type="com:SolarHeatGainCoefficientExteriorShading"/>
         <xsd:element name="statusEAA" type="com:StatusEAA"/>
         <xsd:element name="verticalFinProjection" type="env:VerticalFinProjection"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:simpleType name="ExternalShadingObjectArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="ExternalShadingObjectName">
      <xsd:annotation>
         <xsd:documentation>The name of a shading control that is located on the outside of the building.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:simpleType name="ExternalShadingObjectType">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="DropArmAwnings">Drop arm/Marquisolette awnings</dtyp:displayterm>
            <dtyp:displayterm value="LouveredSunScreen">Louvered sun screens with louvers as wide as openings</dtyp:displayterm>
            <dtyp:displayterm value="LowSunAngleLouveredSunScreen">Low Sun Angle (LSA) louvered sun screens</dtyp:displayterm>
            <dtyp:displayterm value="NoExternalShadingDevice">None</dtyp:displayterm>
            <dtyp:displayterm value="OperableAwnings">Operable awnings</dtyp:displayterm>
            <dtyp:displayterm value="Overhang">Overhang</dtyp:displayterm>
            <dtyp:displayterm value="RetractableAwnings">Retractable awnings</dtyp:displayterm>
            <dtyp:displayterm value="RollDownBlindsOrSlats">Roll down blinds or slats</dtyp:displayterm>
            <dtyp:displayterm value="StandardBugScreen">Standard bug screens</dtyp:displayterm>
            <dtyp:displayterm value="SunScreen">Exterior sunscreens with weave 53x16</dtyp:displayterm>
            <dtyp:displayterm value="VerticalRollerOrShades">Vertical roller or shades</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="DropArmAwnings"/>
         <xsd:enumeration value="LouveredSunScreen"/>
         <xsd:enumeration value="LowSunAngleLouveredSunScreen"/>
         <xsd:enumeration value="NoExternalShadingDevice"/>
         <xsd:enumeration value="OperableAwnings"/>
         <xsd:enumeration value="Overhang"/>
         <xsd:enumeration value="RetractableAwnings"/>
         <xsd:enumeration value="RollDownBlindsOrSlats"/>
         <xsd:enumeration value="StandardBugScreen"/>
         <xsd:enumeration value="SunScreen"/>
         <xsd:enumeration value="VerticalRollerOrShades"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="Fenestration">
      <xsd:annotation>
         <xsd:documentation>Refers to all installed transparent or translucent products that fill openings in the exterior walls and roofs of the building envelope.  Includes all parts of a fenestration product including the glazing material, the frame or curb and dividers.  There are two orientations: Fenestration Vertical and Fenestration Skylight.  Standards: 2013 NR ACM: 5.5.7 Fenestration includes windows, doors that have more than 50% glazed area, and skylights. A skylight is fenestration that has a tilt of less than 60 degrees from horizontal.

T24: FENESTRATION PRODUCT is any transparent or translucent material plus any sash, frame, mullions and dividers, in the envelope of a building, including, but not limited to, windows, sliding glass doors, French doors, skylights, curtain walls, garden windows, and other doors with a glazed area of more than one half of the door area.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence/>
   </xsd:complexType>
   <xsd:simpleType name="FenestrationAdditionType">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="FixedWindow">Fixed window</dtyp:displayterm>
            <dtyp:displayterm value="GlazedDoor">Glazed door</dtyp:displayterm>
            <dtyp:displayterm value="OperableWindow">Operable window</dtyp:displayterm>
            <dtyp:displayterm value="Skylight">Skylight</dtyp:displayterm>
            <dtyp:displayterm value="TubularSkylight">Tubular Skylight</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="FixedWindow"/>
         <xsd:enumeration value="GlazedDoor"/>
         <xsd:enumeration value="OperableWindow"/>
         <xsd:enumeration value="Skylight"/>
         <xsd:enumeration value="TubularSkylight"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationAlterationType">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Add16Sky">Additional skylight area less than or equal to 16 ft2</dtyp:displayterm>
            <dtyp:displayterm value="Add75">Additional vertical fenestration area less than or equal to 75 ft2</dtyp:displayterm>
            <dtyp:displayterm value="Add76">Additional vertical fenestration are greater than 75 ft2</dtyp:displayterm>
            <dtyp:displayterm value="AddSky">Additional skylight area greater than 16 ft2</dtyp:displayterm>
            <dtyp:displayterm value="Repair">Repairing fenestration</dtyp:displayterm>
            <dtyp:displayterm value="FenestrationReplace75">Replaced vertical fenestration area less than or equal to  75 ft2</dtyp:displayterm>
            <dtyp:displayterm value="FenestrationReplaceAll">Replaced vertical fenestration area greater than 75 ft2</dtyp:displayterm>
            <dtyp:displayterm value="FenestrationReplaceSkylight">Replacing Skylight area greater than 16 ft2</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Add16Sky"/>
         <xsd:enumeration value="Add75"/>
         <xsd:enumeration value="Add76"/>
         <xsd:enumeration value="AddSky"/>
         <xsd:enumeration value="Repair"/>
         <xsd:enumeration value="FenestrationReplace75"/>
         <xsd:enumeration value="FenestrationReplaceAll"/>
         <xsd:enumeration value="FenestrationReplaceSkylight"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationCertificationMethod">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="CECDefaultPerformance">Tables 110.6-A and 110.6-B</dtyp:displayterm>
            <dtyp:displayterm value="CF1R_ENV_02">Area-weighted Average Worksheet (CF1R-ENV-02)</dtyp:displayterm>
            <dtyp:displayterm value="NA6Equations">Equations NA 6-1 and NA 6 -2</dtyp:displayterm>
            <dtyp:displayterm value="NFRCRated">NFRC</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="CECDefaultPerformance"/>
         <xsd:enumeration value="CF1R_ENV_02"/>
         <xsd:enumeration value="NA6Equations"/>
         <xsd:enumeration value="NFRCRated"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="FenestrationConstructionBase">
      <xsd:annotation>
         <xsd:documentation>A structure specifying the construction attributes and associated thermal and optical properties that apply to all fenestration products.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="airInfiltrationRate" type="env:AirInfiltrationRate"/>
         <xsd:element name="assemblyContext" type="com:AssemblyContext"/>
         <xsd:element name="dividerMinimumSpacing" type="env:DividerMinimumSpacing"/>
         <xsd:element name="dividerThroughPanes" type="env:DividerThroughPanes"/>
         <xsd:element name="fenestrationConstructionUFactor"
                      type="com:FenestrationConstructionUFactor"/>
         <xsd:element name="visibleTransmittance" type="com:VisibleTransmittance"/>
         <xsd:element name="fenestrationFraming" type="env:FenestrationFraming"/>
         <xsd:element name="framingThermalBreak" type="env:FramingThermalBreak"/>
         <xsd:element name="fenestrationOperability" type="env:FenestrationOperability"/>
         <xsd:element name="glazingMaterial" type="env:GlazingMaterial"/>
         <xsd:element name="glazingTint" type="env:GlazingTint"/>
         <xsd:element name="panesPerLight" type="env:PanesPerLight"/>
         <xsd:element name="sHGC" type="com:SHGC"/>
         <xsd:element name="solarHeatGainCoefficientCEC"
                      type="com:SolarHeatGainCoefficientCEC"/>
         <xsd:element name="sHGC_CenterOfGlass" type="com:SHGC_CenterOfGlass"/>
         <xsd:element name="sHGC_Limit" type="com:SHGC_Limit"/>
         <xsd:element name="solarHeatGainCoefficientNFRC"
                      type="com:SolarHeatGainCoefficientNFRC"/>
         <xsd:element name="uFactorCenterOfGlass" type="com:UFactorCenterOfGlass"/>
         <xsd:element name="uFactorLimit" type="com:UFactorLimit"/>
      </xsd:sequence>
      <xsd:attribute name="fenestrationCertificationMethod"
                     type="env:FenestrationCertificationMethod"
                     use="required"/>
   </xsd:complexType>
   <xsd:complexType name="FenestrationConstructionSkylight">
      <xsd:annotation>
         <xsd:documentation>A structure specialized to specify the major thermal and optical properties of a skylight.  Standards: 2013 NR ACM: No use of this specific term.  5.5.7 Fenestration has an attribute named "Fenestration Type (Skylights)" that is an enumerated list: Glass skylight with curb; plastic skylight with curb; or skylights with no curb.  All Fenestration objects have a property named Fenestration Construction that references a Common Data Structure 5.11.7 Fenestration Construction.

The same attributes of the skylight construction appear in text and tables but there is no formal definition.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="env:FenestrationConstructionBase">
            <xsd:sequence>
               <xsd:element name="skylightCurbType" type="env:SkylightCurbType"/>
            </xsd:sequence>
            <xsd:attribute name="fenestrationConstructionSkylightName"
                           type="env:FenestrationConstructionSkylightName"
                           use="required"/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="FenestrationConstructionSkylightName">
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:complexType name="FenestrationConstructionVertical">
      <xsd:annotation>
         <xsd:documentation>A structure specialized to specify the major thermal and optical properties of any vertical fenestration such as windows, curtain walls, doors with 50% or more glazing.  Standards: 2013 NR ACM: No use of this specific term.  5.5.7 Fenestration has an attribute named "Fenestration Type (Vertical Fenestration)" that is an enumerated list: Fixed or Operable.  All Fenestration objects have a property named Fenestration Construction that references a Common Data Structure 5.11.7 Fenestration Construction.

The same attributes of the vertical construction which is usually termed Window appear in text and tables but there is no formal definition.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="env:FenestrationConstructionBase">
            <xsd:sequence>
               <xsd:element name="relativeSolarHeatGain" type="com:RelativeSolarHeatGain"/>
               <xsd:element name="windowStyle" type="env:WindowStyle"/>
            </xsd:sequence>
            <xsd:attribute name="fenestrationConstructionVerticalName"
                           type="env:FenestrationConstructionVerticalName"
                           use="required"/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="FenestrationConstructionVerticalName">
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:complexType name="FenestrationConstructionsSkylight">
      <xsd:annotation>
         <xsd:documentation>A container of all instances of skylight fenestration construction instances in the Building.  NOTE: Not being assigned to any properties or objects with properties, so should delete when shown Res doesn't need it  Standards: 2013 NR ACM: No use of this specific term.  5.5.7 Fenestration has an attribute named "Fenestration Type (Skylights)" that is an enumerated list: Glass skylight with curb; plastic skylight with curb; or skylights with no curb.  All Fenestration objects have a property named Fenestration Construction that references a Common Data Structure 5.11.7 Fenestration Construction.

The same attributes of the skylight construction appear in text and tables but there is no formal definition.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence/>
   </xsd:complexType>
   <xsd:complexType name="FenestrationConstructionsVertical">
      <xsd:annotation>
         <xsd:documentation>A container of all instances of vertical fenestration construction instances  in the Building. NOTE: Not being assigned to any properties or objects with properties, so should delete when shown Res doesn't need it  Standards: 2013 NR ACM: No use of this specific term.  5.5.7 Fenestration has an attribute named "Fenestration Type (Vertical Fenestration)" that is an enumerated list: Fixed or Operable.  All Fenestration objects have a property named Fenestration Construction that references a Common Data Structure 5.11.7 Fenestration Construction.

The same attributes of the vertical construction which is usually termed Window appear in text and tables but there is no formal definition.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence/>
   </xsd:complexType>
   <xsd:simpleType name="FenestrationFloorRatio">
      <xsd:annotation>
         <xsd:documentation>  Units: percent</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal1PlaceNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationFloorRatioLimitAll">
      <xsd:annotation>
         <xsd:documentation>For residential  new buildings the limit is 20% for all orientations.  The ration/percentage varies with the size of additions.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationFloorRatioLimitWest">
      <xsd:annotation>
         <xsd:documentation>For residential buildings the limit is 5% for west facing fenestration.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationFraming">
      <xsd:annotation>
         <xsd:documentation>The type of frame used for a fenestration product which may be a window, door or curtain wall.</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Fenestration Framing</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="CladFrame">Clad frame</dtyp:displayterm>
            <dtyp:displayterm value="FiberglassFrame">Fiberglass</dtyp:displayterm>
            <dtyp:displayterm value="MetalFraming">Metal</dtyp:displayterm>
            <dtyp:displayterm value="MetalFramingCurtainWall">Metal framing curtain wall</dtyp:displayterm>
            <dtyp:displayterm value="MetalFramingDoor">Metal framing door</dtyp:displayterm>
            <dtyp:displayterm value="MetalFramingOther">Metal framing other</dtyp:displayterm>
            <dtyp:displayterm value="MetalFramingWithThermalBreak">Metal with thermal break</dtyp:displayterm>
