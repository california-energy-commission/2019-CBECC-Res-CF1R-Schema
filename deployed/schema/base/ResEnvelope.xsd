   <xsd:simpleType name="FenestrationIdenticalCount">
      <xsd:annotation>
         <xsd:documentation>  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationName">
      <xsd:restriction base="xsd:string"/>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationNonRatedSiteBuiltTotalAreaAllowed">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationNonRatedSiteBuiltTotalAreaProposed">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationOperability">
      <xsd:annotation>
         <xsd:documentation>The operation capability, if any, of a fenestration product.</xsd:documentation>
