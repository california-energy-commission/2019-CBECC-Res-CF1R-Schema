         <xsd:documentation>The rate of thermal energy transfer expressed in unit of energy per unit of time.  Units: Btu per h</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Thermal Capacity</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TileGapLayer">
      <xsd:restriction base="xsd:string"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalAddedGlazingAreaEastSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing being added for east facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalAddedGlazingAreaNorthSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing being addedor south facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalAddedGlazingAreaSouthSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing being added for south facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalAddedGlazingAreaWestSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing being added for west facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalExistingGlazingAreaAllOrientations">
      <xsd:annotation>
         <xsd:documentation>The sum of all existing fenestration glazing area for all orientations.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalExistingGlazingAreaWestFacing">
      <xsd:annotation>
         <xsd:documentation>The sum of all existing fenestration glazing area for west orientation only  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaAllOrientations">
      <xsd:annotation>
         <xsd:documentation>The sum of all vertical fenestration glazing area for all orientations.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaEast">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing east facing fenestration.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaEastSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The sum of all vertical fenestration glazing area for east facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaNonWestAllProperty">
      <xsd:annotation>
         <xsd:documentation>The sum of all vertical fenestration glazing area for nonwest facing fenestration in the building  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaNorth">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing north facing fenestration.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaNorthSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The sum of all vertical fenestration glazing area for north facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaSkylightSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The sum of all skylight fenestration glazing area for the  same frame and type.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaSouth">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing south facing fenestration.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaSouthSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The sum of all vertical fenestration glazing area for south facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaWest">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing west facing fenestration.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGlazingAreaWestSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The sum of all vertical fenestration glazing area for west facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalNetAddPlusExistingGlazingAreaAllOrientations">
      <xsd:annotation>
         <xsd:documentation>The sum of all existing fenestration glazing area for all orientations plus net addition of  fenestration for all orientations  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalNetAddPlusExistingGlazingAreaWestFacing">
      <xsd:annotation>
         <xsd:documentation>The sum of all existing fenestration glazing area for west orientation plus net addition of west facing fenestration  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalRemovedGlazingAreaEastSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing being removed or replaced for east facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalRemovedGlazingAreaNorthSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing being removed or replaced for south facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalRemovedGlazingAreaSouthSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing being removed or replaced for south facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalRemovedGlazingAreaWestSameTypeAndFrame">
      <xsd:annotation>
         <xsd:documentation>The total area for all vertical fenestration glazing being removed or replaced for west facing fenestration of the  same frame and type (window or glazed door)  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalSkylightArea">
      <xsd:annotation>
         <xsd:documentation>The sum of all skylights in a building.  Total Skylight Area can be derived from the Fenestration Skylight object which contains all skylights in the building.  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Total Skylight Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="VerticalFinProjection">
      <xsd:annotation>
         <xsd:documentation>This is the horizontal distance, measured outward horizontally from the surface
of exposed exterior glazing at the jamb of a window to the outward edge of a fin.  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="ViewFactorToGround">
      <xsd:annotation>
         <xsd:documentation>The fraction of the ground plane which is visible from a heat transfer surface.  Note: Values of this type can be explicitly entered as a dimensionless ratio from 0 to 1 or as one of the special values "AutoCalculate" or "Unassigned".  Therefore, this type is a basic string data type.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token"/>
   </xsd:simpleType>
   <xsd:simpleType name="VisibleReflectance">
      <xsd:annotation>
         <xsd:documentation>From E+ Input/Output Ref:The diffuse visible reflectance of the unglazed part of the shading surface (default = 0.2). This reflectance is assumed to be the same for beam-to-diffuse and diffuse-to-diffuse reflection. Beam-to-diffuse reflection is assumed to be independent of angle of incidence of beam radiation. Diffuse-to-diffuse reflection is assumed to be independent of angular distribution of the incident of diffuse radiation. The outgoing diffuse radiation is assumed to be isotropic (hemispherically uniform).  This value if used only for the daylighting calculation (ref: Daylighting:Controls).  The sum of this reflectance and the shading surface transmittance should be less than or equal to 1.0.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="WWR">
      <xsd:annotation>
         <xsd:documentation>The ratio of the rough opening area of the window in a parent Wall to the total Wall surface area.  This can either be a local ratio (for a single wall) or a global ratio (for all walls in the building or project).  Units: none</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Window Wall Ratio</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="WallFramingMaterialCBECC_Res">
      <xsd:annotation>
         <xsd:documentation>This property is for values in the CF1R-PRF from CBECC-Res reported in the CF2R-ENV-03</xsd:documentation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="SIPS_Walls">SIPS  Walls</dtyp:displayterm>
            <dtyp:displayterm value="SteelFramedWalls">Steel Framed Walls</dtyp:displayterm>
            <dtyp:displayterm value="WoodFramedWalls">Wood Framed Walls</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="SIPS_Walls"/>
         <xsd:enumeration value="SteelFramedWalls"/>
         <xsd:enumeration value="WoodFramedWalls"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="WestFacingFenestrationRange">
      <xsd:annotation>
         <xsd:documentation>  Units: deg arc</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative">
         <xsd:maxInclusive value="315"/>
         <xsd:minInclusive value="225"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="WindExposure">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="NoWind">NoWind</dtyp:displayterm>
            <dtyp:displayterm value="WindExposed">Wind exposed</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="NoWind"/>
         <xsd:enumeration value="WindExposed"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="Window">
      <xsd:annotation>
         <xsd:documentation>Vertical fenestration installed an opening in an exterior wall.  It may be a window, curtain wall, garden window and a door with 50% or more glazing.  Standards: 2013 T24 glossary definition:  WINDOW is fenestration that is not a skylight and that is an assembled unit consisting of a frame and sash component holding one or more pieces of glazing</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="fenestrationVerticalArea" type="env:FenestrationVerticalArea"/>
         <xsd:element name="fenestrationVerticalCount" type="env:FenestrationVerticalCount"/>
         <xsd:element name="height" type="dtyp:Height"/>
         <xsd:element name="overhangDepth" type="com:OverhangDepth"/>
         <xsd:element name="rightFinAngle" type="com:RightFinAngle"/>
         <xsd:element name="statusEAA" type="com:StatusEAA"/>
         <xsd:element name="width" type="dtyp:Width"/>
         <xsd:element name="windowArea" type="com:WindowArea"/>
         <xsd:element name="xCoordinate" type="dtyp:XCoordinate"/>
         <xsd:element name="yCoordinate" type="dtyp:YCoordinate"/>
         <xsd:element name="ExternalShadingObject" type="env:ExternalShadingObject"/>
         <xsd:element name="FenestrationSurfaceGroup" type="env:FenestrationSurfaceGroup"/>
         <xsd:element name="InteriorShadingDevice" type="env:InteriorShadingDevice"/>
         <xsd:element name="OpeningGeometry" type="env:OpeningGeometry"/>
      </xsd:sequence>
      <xsd:attribute name="fenestrationConstructionVerticalName"
                     type="env:FenestrationConstructionVerticalName"
                     use="required"/>
      <xsd:attribute name="fenestrationVerticalName"
                     type="env:FenestrationVerticalName"
                     use="required"/>
      <xsd:attribute name="improvementStatus"
                     type="com:ImprovementStatus"
                     use="required"/>
   </xsd:complexType>
   <xsd:simpleType name="WindowConstructionName">
      <xsd:annotation>
         <xsd:documentation>The name of a construction assembly which references a special material (WindowMaterialSimpleGlazingSystem)</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:simpleType name="WindowFloorRatio">
      <xsd:annotation>
         <xsd:documentation>  Units: none</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Window Floor Ratio</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="WindowFrameAndDividerName">
      <xsd:restriction base="dtyp:ObjectNamePermissive"/>
   </xsd:simpleType>
   <xsd:simpleType name="WindowStyle">
      <xsd:annotation>
         <xsd:documentation>Defines whether the fenestration construction is a garden window, glazed door, window or other type of vertical fenestration.</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Window Style</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="GardenWindow">Garden window</dtyp:displayterm>
            <dtyp:displayterm value="GlazedDoor">Glazed door</dtyp:displayterm>
            <dtyp:displayterm value="WindowOther">Window</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="GardenWindow"/>
         <xsd:enumeration value="GlazedDoor"/>
         <xsd:enumeration value="WindowOther"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="WindowSystemName">
      <xsd:restriction base="xsd:token"/>
   </xsd:simpleType>
   <xsd:complexType name="WoodFramedConstructionAssemblyFloor">
      <xsd:annotation>
         <xsd:documentation>2013  Joint Appendix 4: tables: 4.4.1, 4.4.2</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="floorType" type="env:FloorType"/>
         <xsd:element name="framingSize" type="env:FramingSize"/>
         <xsd:element name="framingSpacing" type="env:FramingSpacing"/>
         <xsd:element name="insulationCavityRValue" type="env:InsulationCavityRValue"/>
         <xsd:element name="insulationContinuousRValue" type="env:InsulationContinuousRValue"/>
         <xsd:element name="partitionFramingMaterial" type="env:PartitionFramingMaterial"/>
         <xsd:element name="uFactorLimit" type="com:UFactorLimit"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="WoodFramedConstructionAssemblyRoof">
      <xsd:annotation>
         <xsd:documentation>2013  Joint Appendix 4: tables: 4.2.1, 4.2.2,</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="framingSize" type="env:FramingSize"/>
         <xsd:element name="framingSpacing" type="env:FramingSpacing"/>
         <xsd:element name="insulationCavityRValue" type="env:InsulationCavityRValue"/>
         <xsd:element name="insulationContinuousRValue" type="env:InsulationContinuousRValue"/>
         <xsd:element name="partitionFramingMaterial" type="env:PartitionFramingMaterial"/>
         <xsd:element name="roofType" type="env:RoofType"/>
         <xsd:element name="uFactorLimit" type="com:UFactorLimit"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="WoodFramedConstructionAssemblyWall">
      <xsd:annotation>
         <xsd:documentation>2013  Joint Appendix 4: tables:4.3.1</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="framingSize" type="env:FramingSize"/>
         <xsd:element name="framingSpacing" type="env:FramingSpacing"/>
         <xsd:element name="insulationCavityRValue" type="env:InsulationCavityRValue"/>
         <xsd:element name="insulationContinuousRValue" type="env:InsulationContinuousRValue"/>
         <xsd:element name="partitionFramingMaterial" type="env:PartitionFramingMaterial"/>
         <xsd:element name="uFactorLimit" type="com:UFactorLimit"/>
      </xsd:sequence>
   </xsd:complexType>
</xsd:schema>
