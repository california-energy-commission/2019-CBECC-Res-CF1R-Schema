            <dtyp:displayterm>Fenestration Framing Thermal Break</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="NoThermalBreak">No thermal break</dtyp:displayterm>
            <dtyp:displayterm value="ThermalBreak">Thermal break</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="NoThermalBreak"/>
         <xsd:enumeration value="ThermalBreak"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FurringStripDescriptionCBECC_Res">
