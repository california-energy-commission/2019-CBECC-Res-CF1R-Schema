            <dtyp:displayterm value="48InchOC">@ 48 in. O. C.</dtyp:displayterm>
            <dtyp:displayterm value="96InchOC">@ 96 in. O. C.</dtyp:displayterm>
            <dtyp:displayterm value="Other"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstantPermissive">
         <xsd:enumeration value="16InchOC"/>
         <xsd:enumeration value="24InchOC"/>
         <xsd:enumeration value="48InchOC"/>
         <xsd:enumeration value="96InchOC"/>
         <xsd:enumeration value="Other"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FramingSpacingOther">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Framing Spacing Other</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:token"/>
