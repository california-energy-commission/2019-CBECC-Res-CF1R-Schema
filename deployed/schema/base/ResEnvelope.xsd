         <xsd:enumeration value="FiberglassFrame"/>
         <xsd:enumeration value="MetalFraming"/>
         <xsd:enumeration value="MetalFramingCurtainWall"/>
         <xsd:enumeration value="MetalFramingDoor"/>
         <xsd:enumeration value="MetalFramingOther"/>
         <xsd:enumeration value="MetalFramingWithThermalBreak"/>
         <xsd:enumeration value="NoFrame"/>
         <xsd:enumeration value="NonMetalFraming"/>
         <xsd:enumeration value="ReinforcedVinylAluminumCladWood"/>
         <xsd:enumeration value="VinylFrame"/>
         <xsd:enumeration value="WoodFrame"/>
